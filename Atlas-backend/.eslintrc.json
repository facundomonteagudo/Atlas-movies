{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": "off",
    "comma-dangle": ["error", "never"],
    "newline-per-chained-call": ["error", { "ignoreChainWithDepth": 5 }],
    "consistent-return": "off"
  }
}
